{
  "name": "pingem-sdk",
  "version": "0.1.11",
  "description": "Lightweight event tracking for Farcaster Mini Apps",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/index.global.js",
  "jsdelivr": "./dist/index.global.js",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "default": "./dist/index.global.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsup",
    "test": "vitest",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs"
  },
  "files": [
    "dist",
    "src",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "analytics",
    "farcaster",
    "tracking",
    "sdk",
    "frames",
    "pingem",
    "farcaster-miniapps"
  ],
  "author": "Your Name",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.6.12",
    "tsup": "^7.2.0",
    "tsx": "^4.0.0",
    "typescript": "^5.8.3",
    "vitepress": "^1.0.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "node-fetch": "^3.3.2",
    "undici": "^7.8.0"
  }
}
